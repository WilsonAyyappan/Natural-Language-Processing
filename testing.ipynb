    "import requests\n",
    "\n",
    "def test_model_service(text):\n",
    "    url = 'http://0.0.0.0:5001/predict'\n",
    "    data = {'text': text}\n",
    "    response = requests.post(url, json=data)\n",
    "    return response.json()\n",
    "\n",
    "# Test the service\n",
    "test_text = \"Text to test the Endpoint\"\n",
    "print(test_model_service(test_text))\n",
    "\n"
